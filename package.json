{
  "name": "vite-plugin-svg-icon-types",
  "version": "0.2.3",
  "private": false,
  "type": "module",
  "description": "Vite plugin that generates typed svg icon names and a preview page.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node scripts/build.mjs",
    "build:watch": "node scripts/watch.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vite-plugin",
    "svg",
    "icons",
    "types",
    "preview"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "esbuild": "^0.23.0",
    "rollup": "^4.53.3",
    "typescript": "^5.3.3",
    "vite": "^5.2.0"
  }
}
